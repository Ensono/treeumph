{
  "name": "treeumph",
  "version": "1.0.0",
  "description": "Repository to hold the app to donate and plant a tree for every Kudos given on Bob/Slack",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "build": "npm run lint && tsc",
    "lint": "tsc --noEmit && npm run check && npm run format",
    "check": "rome check .",
    "check:fix": "rome check . --apply",
    "format": "rome format .",
    "format:fix": "rome format . --write",
    "dev": "nodemon --exec ts-node src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amido/treeumph.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amido/treeumph/issues"
  },
  "homepage": "https://github.com/amido/treeumph#readme",
  "dependencies": {
    "@slack/bolt": "^3.12.2",
    "axios": "^1.2.1",
    "date-fns": "^2.29.3",
    "pino": "^8.7.0"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.25.0",
    "@types/aws-lambda": "^8.10.109",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^18.11.15",
    "dot": "^1.1.3",
    "dotenv": "^16.0.3",
    "esbuild": "^0.16.7",
    "nodemon": "^2.0.20",
    "rome": "^11.0.0",
    "serverless": "^3.25.1",
    "serverless-esbuild": "^1.34.0",
    "serverless-offline": "^12.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  }
}
